{
    "extends" : ["airbnb", "prettier", "plugin:node/recommend"],
    "plugins" : ["prettier"],
    "rules" : {
        "prettier/prettier" : "error",
        "spaced-comment": "off",
        "no-console" : "warn",
        "consistent-return" : "off",
        "func-names" : "off",
        "object-shorthand" : "off",
        "no-process-exit" : "off",
        "no-params-reasign": "off",
        "no-return-await" : "off",
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off",
        "prefer-destructuring": ["error", { "object": true, "array": false}],
        "no-unsued-vars" : ["error", { "argsIgnorePattern" : "req|res|next|val"}]

    }
}